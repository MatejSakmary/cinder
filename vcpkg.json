{
  "name": "sandbox",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "daxa",
      "features": [
        "utils-pipeline-manager-glslang",
        "utils-pipeline-manager-slang",
        "utils-task-graph",
        "utils-imgui"
      ]
    },
    "glfw3",
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "docking-experimental"
      ]
    },
    "glm",
    "meshoptimizer",
    "fastgltf",
    "fmt",
    "ktx",
    "freeimage",
    "nlohmann-json",
    "implot"
  ],
  "builtin-baseline": "e56312cb3d33be09ec550ade6ffbb8dc18d23cfd",
  "vcpkg-configuration": {
    "overlay-ports": [
      "deps/daxa",
      "deps/daxa/cmake/vcpkg-overlay-ports/shader-slang"
    ]
  }
}
